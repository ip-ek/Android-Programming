<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ipk.mobilodev">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.ipk.mobilodev.SensorActivity" />
        <activity android:name="com.ipk.mobilodev.SettingsActivity" />
        <activity android:name="com.ipk.mobilodev.PersonActivity" />
        <activity android:name="com.ipk.mobilodev.NotesActivity" />
        <activity android:name="com.ipk.mobilodev.MailActivity" />
        <activity android:name="com.ipk.mobilodev.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.ipk.mobilodev.MenuActivity" />
    </application>

</manifest>